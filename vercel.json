{
  "version": 2,
  "builds": [
    {
      "src": "./package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/Move-In-Precise-Speed.mp4",
      "dest": "/Move-In-Precise-Speed.mp4"
    },
    {
      "src": "/Icon.png",
      "dest": "/Icon.png"
    },
    {
      "src": "/fallbackimage.png",
      "dest": "/fallbackimage.png"
    },
    {
      "src": "/logo192.png",
      "dest": "/logo192.png"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/robots.txt",
      "dest": "/robots.txt"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 